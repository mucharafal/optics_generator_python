export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$path_to_SWAN_Projects/optics_generator_python/root_libs:/cvmfs/sft.cern.ch/lcg/views/LCG_96python3/x86_64-centos7-gcc8-opt/lib: